<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.va.vba.persistence.models.data">

	<class name="Claims" table="AH4929_RATING_CORP_CLAIM">

		<id name="veteranId" column="PTCPNT_VET_ID" type="long" >
			<generator class="native"/>
		</id>

		<property name="prfillDate" column="PRFIL_DT" type="date" />
		<property name="claimId" column="BNFT_CLAIM_ID" type="long" />
		<property name="endPrdctTypeCode" column="END_PRDCT_TYPE_CD" type="string" />
		<property name="claimDate" column="DATE_OF_CLAIM" type="date" />
		<property name="payeeTypeCode" column="PAYEE_TYPE_CD" type="string" />
		<property name="claimTypeCode" column="BNFT_CLAIM_TYPE_CD" type="string" />
		<property name="claimLabel" column="CLAIM_LABEL" type="string" />
		<property name="statusTypeCd" column="STATUS_TYPE_CD" type="string" />
		<property name="claimRegionalOfficeNumber" column="CLAIM_RO_NUMBER" type="long" />
		<property name="regionalOfficeOfClaim" column="CLAIM_RO_NAME" type="string" />
		<property name="contentionId" column="CNTNTN_ID" type="string" />
		<property name="contentionClsfcnId" column="CNTNTN_CLSFCN_ID" type="long" />
		<property name="contentionTypeCode" column="CNTNTN_TYPE_CD" type="string" />
		<property name="contentionClaimTextKeyForModel" column="CNTNTN_CLMANT_TXT" type="string" />
		<property name="contentionMedInd" column="CNTNTN_MED_IND" type="string" />
		<property name="contentionWellGrndedAplcblInd" column="CNTNTN_WELL_GRNDED_APLCBL_IND" type="string" />
		<property name="contentionBeginDate" column="CNTNTN_BEGIN_DT" type="date" />
		<property name="contentionSpeclIssueId" column="CNTNTN_SPECIAL_ISSUE_ID" type="string" />
		<property name="contentionSpeclIssueTypeCode" column="CNTNTN_SPECIAL_ISSUE_TYPE_CD" type="string" />
		
		<set name="veterans" table="AH4929_PERSON" fetch="select" cascade="all">
			<key column="PTCPNT_VET_ID" />
			<many-to-many class="Veterans" column="PTCPNT_VET_ID" />
		</set>
		
    </class>

</hibernate-mapping>
