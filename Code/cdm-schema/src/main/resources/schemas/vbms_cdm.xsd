<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://vbms.vba.va.gov/cdm"
    targetNamespace="http://vbms.vba.va.gov/cdm" elementFormDefault="qualified" version="1.2">
    <xs:complexType name="Address">
        <xs:sequence>
            <xs:element name="source" type="Source" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="addressLine1" type="xs:string" use="required"/>
        <xs:attribute name="addressLine2" type="xs:string" use="optional"/>
        <xs:attribute name="addressLine3" type="xs:string" use="optional"/>
        <xs:attribute name="AddressType_ID" type="xs:string"/>
        <xs:attribute name="city" type="xs:string"/>
        <xs:attribute name="State_Code" type="xs:string"/>
        <xs:attribute name="Country_Code" type="xs:string"/>
        <xs:attribute name="postalCode" type="xs:string"/>
        <xs:attribute name="preferredAddrInd" type="xs:boolean" use="required"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="AlternateName">
        <xs:sequence>
            <xs:element name="source" type="Source" minOccurs="0"/>
            <xs:element name="namePrefix" type="NamePrefix" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="firstName" type="xs:string" use="required"/>
        <xs:attribute name="middleName" type="xs:string"/>
        <xs:attribute name="lastName" type="xs:string" use="required"/>
        <xs:attribute name="NameSuffix_ID" type="xs:long"/>
    </xs:complexType>
    <xs:complexType name="Annotation">
        <xs:sequence>
            <xs:element name="content" type="xs:string"/>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="documentID" type="xs:string" use="required"/>
        <xs:attribute name="text" type="xs:string"/>
        <xs:attribute name="type" type="xs:string" use="required"/>
        <xs:attribute name="createUser_ID" type="xs:long" use="required"/>
        <xs:attribute name="modifiedUser_ID" type="xs:long"/>
        <xs:attribute name="jpaVersion" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="Claim">
        <xs:sequence>
            <xs:element name="EndProductClaimType" type="CodeValue">
                <xs:annotation>
                    <xs:documentation>List of VETSNET ClaimTypeCode,ClaimTypeName pairs</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="claimDateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="flashList" type="Flash" maxOccurs="5000" minOccurs="0"/>
            <xs:element name="suspenseDate" type="xs:date" minOccurs="0"/>
            <xs:element name="contentions" type="Contention" minOccurs="0" maxOccurs="5000"/>
            <xs:element name="inTakeSite" type="xs:string" minOccurs="0"/>
            <xs:element name="suppressAckLetterInd" type="xs:boolean" default="false" minOccurs="0"/>
            <xs:element name="futureReason" type="xs:string" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="Participant_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="PowerOfAttorney_ID" type="xs:string"/>
        <xs:attribute name="Veteran_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="InsuranceInfo_ID" type="xs:long"/>
        <xs:attribute name="benefitTypeCd" type="xs:string">
            <xs:annotation>
                <xs:documentation>Maps to BenefitType - Live, Death, CH31</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="claimLevelStatusCd" type="xs:string" use="required"/>
        <xs:attribute name="modifiedEndProductCd" type="xs:string"/>
        <xs:attribute name="gulfWarRegistryInd" type="xs:boolean"/>
        <xs:attribute name="label" type="xs:string" use="required"/>
        <xs:attribute name="payeeCd" type="xs:string" use="required"/>
        <xs:attribute name="sectionUnit" type="xs:string"/>
        <xs:attribute name="serviceTypeCd" type="xs:string"/>
        <xs:attribute name="specialIssueCd" type="xs:string"/>
        <xs:attribute name="stationOfJurisdiction" type="xs:string" use="required"/>
        <xs:attribute name="currentStationOfJurisdiction" type="xs:string"/>
        <xs:attribute name="suspenseReasonCd" type="xs:string"/>
        <xs:attribute name="priority" type="xs:string" use="required"/>
        <xs:attribute name="td" type="xs:string"/>
        <xs:attribute name="disposition" type="xs:string"/>
        <xs:attribute name="folderWithClaim" type="xs:string"/>
        <xs:attribute name="preDischargeInd" type="xs:boolean" default="false"/>
    </xs:complexType>

    <xs:complexType name="CodeValue">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="expirationDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="code" type="xs:string" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>

    </xs:complexType>
    <xs:complexType name="Contention">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="completionDate" type="xs:date" minOccurs="0"/>
            <xs:element name="source" type="Source" minOccurs="0"/>
            <xs:element name="submitDate" type="xs:date"/>
            <xs:element name="issue" minOccurs="0" type="Issue" maxOccurs="5000">
                <xs:annotation>
                    <xs:documentation>Associations between Contentions and Issue are maintained by the
                        ISSUE_CONTENTION_ASSOC table in the PDM.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="startDate" type="xs:date"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:string" use="required"/>
        <xs:attribute name="Claim_ID" type="xs:long" use="required"/>
        <xs:attribute name="actionableItemInd" type="xs:boolean"/>
        <xs:attribute name="awaitingResponse" type="xs:string" use="required"/>
        <xs:attribute name="classificationCd" type="xs:string"/>
        <xs:attribute name="decisionHistoryCd" type="xs:string"/>
        <xs:attribute name="decisionHistoryNotes" type="xs:string"/>
        <xs:attribute name="decisionStatusCode" type="xs:string"/>
        <xs:attribute name="fileNumber" type="xs:string"/>
        <xs:attribute name="levelStatusCode" type="xs:string"/>
        <xs:attribute name="medicalInd" type="xs:boolean" use="required"/>
        <xs:attribute name="partcipantContention" type="xs:string" use="required"/>
        <xs:attribute name="secondaryToContentionID" type="xs:string" use="required"/>
        <xs:attribute name="title" type="xs:string" use="required"/>
        <xs:attribute name="typeCode" type="xs:string"/>
        <xs:attribute name="workingContention" type="xs:string" use="required"/>
        <xs:attribute name="contentionCategory" type="ContentionCategory"/>
    </xs:complexType>
    <xs:complexType name="CriminalInfo">
        <xs:sequence>
            <xs:element name="dateSentToJail" type="xs:date"/>
            <xs:element name="dateReleasedFromJail" type="xs:date"/>
            <xs:element name="paroleEndDt" type="xs:date"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="PersonalInfo_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="crimeCommitted" type="xs:string" use="required"/>
        <xs:attribute name="verdict" type="xs:string" use="required"/>
        <xs:attribute name="sentence" type="xs:string" use="required"/>
        <xs:attribute name="stateOfCrime" type="xs:string"/>
        <xs:attribute name="countryOfCrime" type="xs:string"/>
        <xs:attribute name="onParoleInd" type="xs:boolean" use="required"/>
        <xs:attribute name="prisonName" type="xs:string"/>
        <xs:attribute name="cityOfCrime" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Decoration">
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="awardDt" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Department">
        <xs:complexContent>
            <xs:extension base="LookUpValue">
                <xs:sequence>
                    <xs:element name="createDate" type="xs:date"/>
                    <xs:element name="lastUpdateDate" type="xs:date" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DeploymentInfo">
        <xs:sequence>
            <xs:element name="inTheaterStartDt" type="xs:date"/>
            <xs:element name="inTheaterEndDt" type="xs:date"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="ServiceInfo_ID" type="xs:long" use="required"/>
        <xs:attribute name="inTheaterDays" type="xs:int" use="required"/>
    </xs:complexType>
    <xs:simpleType name="DocumentId">
        <xs:restriction base="xs:string">
            <xs:pattern value="\{[A-F0-9]{8}-[A-F0-9]{4}-[A-F0-9]{4}-[A-F0-9]{4}-[A-F0-9]{12}\}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FileNumber">
        <xs:restriction base="xs:string">
            <xs:minLength value="8"/>
            <xs:maxLength value="9"/>
            <xs:pattern value="[0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Document">
        <xs:sequence>
            <xs:element maxOccurs="5000" minOccurs="0" name="annotation" type="Annotation"> </xs:element>
            <xs:element name="docContent" type="xs:base64Binary" minOccurs="0"/>
            <xs:element name="trackedItems" type="TrackedItem"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
            <xs:element name="receivedDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:string" use="required"/>
        <xs:attribute name="actionable" type="xs:boolean" use="required"/>
        <xs:attribute name="externalID" type="xs:string" use="required"/>
        <xs:attribute name="Veteran_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="path" type="xs:string" use="required"/>
        <xs:attribute name="fileNumber" type="xs:string" use="required"/>
        <xs:attribute name="filename" type="xs:string" use="required"/>
        <xs:attribute name="createUser_ID" type="xs:string" use="required"/>
        <xs:attribute name="category" type="xs:string"/>
        <xs:attribute name="docType" type="xs:string" use="required"/>
        <xs:attribute name="metadata" type="xs:string"/>
        <xs:attribute name="newMail" type="xs:boolean" use="required"/>
        <xs:attribute name="mimeType" type="xs:string" use="required"/>
        <!--needs inspection-->
        <xs:attribute name="subject" type="xs:string"/>
        <xs:attribute name="endProduct" type="xs:string"/>
        <xs:attribute name="source" type="xs:string"/>
        <xs:attribute name="shippingNum" type="xs:string"/>
        <xs:attribute name="veteranFirstName" type="xs:string"/>
        <xs:attribute name="veteranMiddleName" type="xs:string"/>
        <xs:attribute name="veteranLastName" type="xs:string"/>
        <xs:attribute name="veteranSuffix" type="xs:string"/>
        <xs:attribute name="dcsId" type="xs:string"/>
    </xs:complexType>
    <xs:simpleType name="DocumentAssociationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CLAIM"/>
            <xs:enumeration value="CONTENTION"/>
            <xs:enumeration value="DOCUMENT" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="DocumentAssociation">
        <xs:complexContent>
            <xs:extension base="DocumentAssociationInput">
                <xs:sequence>
                    <xs:element minOccurs="1" name="createDt" type="xs:date"/>
                    <xs:element minOccurs="1" name="lastUpdateDt" type="xs:date"/>
                    <xs:element minOccurs="0" name="associatedDocType" type="xs:string" />
                </xs:sequence>
                <xs:attribute name="ID" type="xs:long" use="required"/>
                <xs:attribute name="documentId" type="DocumentId" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DocumentAssociationInput">
        <xs:sequence>
            <xs:element name="associationValue" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="associationType" type="DocumentAssociationType" use="required"/>
    </xs:complexType>
    <xs:complexType name="LetterDocument">
        <xs:complexContent>
            <xs:extension base="Document">
                <xs:attribute name="claimId" type="xs:string"/>
                <xs:attribute name="letterId" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DocumentType">
        <xs:complexContent>
            <xs:extension base="LookUpValue">
                <xs:attribute name="filenetDocumentClass" type="xs:string"/>
                <xs:attribute name="cdmDocumentClass" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="Email">
        <xs:sequence>
            <xs:element name="source" type="Source"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="PersonalInfo_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="eMail" type="xs:string" use="required"/>
        <xs:attribute name="EmailType" type="xs:string"/>
        <xs:attribute name="notes" type="xs:string"/>
        <xs:attribute name="preferredEmailInd" type="xs:boolean" use="required"/>
    </xs:complexType>
    <xs:complexType name="FaultDetailBean">
        <xs:attribute name="exceptionClassName" type="xs:string"/>
        <xs:attribute name="message" type="xs:string" use="required"/>
        <xs:attribute name="uid" type="xs:string"/>
        <xs:attribute name="serverException" type="xs:boolean" use="required"/>
    </xs:complexType>
    <xs:element name="faultDetailBean" type="FaultDetailBean"/>
    <xs:complexType name="FinancialInfo">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="PersonalInfo_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="disabilitySeverancePay" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="lumpReadjPay" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="netWorthAmnt" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="substananceAmnt" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:decimal">
                    <xs:fractionDigits value="2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="taxAbatement" type="xs:string"/>
        <xs:attribute name="taxID" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Flash">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="assignedInd" type="xs:string" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="type" type="xs:string" use="required"/>
        <xs:attribute name="ID" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="FormDocument">
        <xs:complexContent>
            <xs:extension base="Document">
                <xs:sequence>
                    <xs:element maxOccurs="5000" minOccurs="0" name="formFields"
                        type="FormField"/>
                </xs:sequence>
                <xs:attribute name="dcsScanningComplete" type="xs:boolean" default="false"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="FormField">
        <xs:sequence>
            <xs:element name="value"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="maxLength" type="xs:long"/>
        <xs:attribute name="required" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="InsuranceInfo">
        <xs:sequence>
            <xs:element name="policyStartDt" type="xs:date"/>
            <xs:element name="policyEndDt" type="xs:date"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="PersonalInfo_Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="providerName" type="xs:string" use="required"/>
        <xs:attribute name="insuredName" type="xs:string" use="required"/>
        <xs:attribute name="policyPrefix" type="xs:string" use="required"/>
        <xs:attribute name="policyNumber" type="xs:string" use="required"/>
        <xs:attribute name="policyGroupNumber" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="Issue">
        <xs:sequence>
            <xs:element name="specificRating" maxOccurs="5000" type="xs:string" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="contentionID" type="xs:string" use="required"/>
        <xs:attribute name="inferredInd" type="xs:boolean"/>
        <xs:attribute name="narrative" type="xs:string"/>
        <xs:attribute name="typeCd" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="LookUpValue">
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="description" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="NamePrefix">
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="prefix" type="xs:string" use="required"/>
        <xs:attribute name="bgsInd" type="xs:boolean" use="required"/>
    </xs:complexType>
    <xs:complexType name="Participant">
        <xs:complexContent>
            <xs:extension base="Person">
                <xs:attribute name="externalPartID" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="Party" abstract="true">
        <xs:sequence>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
    </xs:complexType>
    <xs:complexType name="Person">
        <xs:complexContent>
            <xs:extension base="Party">
                <xs:sequence>
                    <xs:element name="alternateNames" maxOccurs="5000" type="AlternateName"
                        minOccurs="0"/>
                    <xs:element name="personalInfo" type="PersonalInfo" minOccurs="0"/>
                    <xs:element name="namePrefix" type="NamePrefix" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="firstName" type="xs:string" use="required"/>
                <xs:attribute name="middleName" type="xs:string"/>
                <xs:attribute name="lastName" type="xs:string" use="required"/>
                <xs:attribute name="gender" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="M"/>
                            <xs:enumeration value="F"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="NameSuffix_ID" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PersonalInfo">
        <xs:sequence>
            <xs:element name="address" minOccurs="0" type="Address" maxOccurs="5000">
                <xs:annotation>
                    <xs:documentation>Associations between Address and PersonalInfo are maintained by the
                        PERSONALINFO_ADDRESS_ASSOC table in the PDM.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="eMail" type="Email" maxOccurs="5000" minOccurs="0"/>
            <xs:element name="criminalInfo" type="CriminalInfo" minOccurs="0" maxOccurs="5000"/>
            <xs:element name="financialInfo" type="FinancialInfo" minOccurs="0"
                maxOccurs="5000"/>
            <xs:element name="insuranceInfo" type="InsuranceInfo" minOccurs="0"
                maxOccurs="5000"/>
            <xs:element name="phone" type="Phone" minOccurs="0" maxOccurs="5000">
                <xs:annotation>
                    <xs:documentation>Associations between Phone and PersonalInfo are maintained by the
                        PERSONALINFO_PHONE_ASSOC table in the PDM.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateOfBirth" type="xs:date" minOccurs="0"/>
            <xs:element name="dateOfDeath" type="xs:date" minOccurs="0"/>
            <xs:element name="currentOccupationStartDt" type="xs:date" minOccurs="0"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="Person_ID" type="xs:long" use="required"/>
        <xs:attribute name="birthCity" type="xs:string"/>
        <xs:attribute name="birthState_Code" type="xs:string"/>
        <xs:attribute name="birthCountry_Code" type="xs:string"/>
        <xs:attribute name="causeOfDeath" type="xs:string"/>
        <xs:attribute name="ssn" type="xs:string"/>
        <xs:attribute name="ssnValidation" type="xs:string"/>
        <xs:attribute name="noSsnReason" type="xs:string"/>
        <xs:attribute name="employmentInd" type="xs:boolean"/>
        <xs:attribute name="currentOccupation" type="xs:string"/>
        <xs:attribute name="potentialDangersInd" type="xs:boolean"/>
        <xs:attribute name="employmenHdcpInd" type="xs:boolean"/>
        <xs:attribute name="birthplaceValidation" type="xs:string"/>
        <xs:attribute name="dobValidation" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="Phone">
        <xs:sequence>
            <xs:element name="source" type="Source"/>
            <xs:element name="createDt" type="xs:date" minOccurs="0"/>
            <xs:element name="lastUpdateDt" type="xs:date" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:long" use="required"/>
        <xs:attribute name="phoneNumber" type="xs:string" use="required"/>
        <xs:attribute name="timeToCall" type="TimeToCall"/>
        <xs:attribute name="notes" type="xs:string"/>
        <xs:attribute name="PhoneType_ID" type="xs:string"/>
        <xs:attribute name="preferredPhoneInd" type="xs:boolean" use="required"/>
    </xs:complexType>
    <xs:simpleType name="TimeToCall">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Day Time"/>
            <xs:enumeration value="Night Time"/>
        </xs:restriction>
